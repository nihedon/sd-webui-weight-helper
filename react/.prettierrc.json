{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 160,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "^preact$",
        "^preact/(.*)$",
        "^@/shared(/.*)?$",
        "^@/components(/.*)?$",
        ".*"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
